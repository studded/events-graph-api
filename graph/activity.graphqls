type Activity {
  id: ID!
	event: Event!
  name: String!
  startTime: String!
  endTime: String!
	description: String!
}

input NewActivity {
  eventId: ID!
	name: String!
	startTime: String!
	endTime: String!
	description: String!
}

input UpdateActivity {
	name: String
	startTime: String
	endTime: String
	description: String
}