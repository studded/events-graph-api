type Event {
  id: ID!
  name: String!
  startDate: String!
  endDate: String!
  location: String!
  description: String!
  activities: [Activity!]!
	roles: [Role!]!
	expenses: [Expense!]!
}

input NewEvent {
	name: String!
	startDate: String!
	endDate: String!
	location: String!
	description: String!
}

input UpdateEvent {
	name: String
	startDate: String
	endDate: String
	location: String
	description: String
}
