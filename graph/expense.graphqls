type Expense {
  id: ID!
	event: Event!
  name: String!
	cost: Float!
	description: String!
	category: String!
}

input NewExpense {
  eventId: ID!
	name: String!
	cost: Float!
	description: String!
	category: String!
}

input UpdateExpense {
	name: String
	cost: Float
	description: String
	category: String
}

type ExpenseCategory {
	category: String!
	total: Float!
}